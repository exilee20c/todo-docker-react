{
  "name": "todo-docker-react",
  "version": "1.0.2",
  "description": "todo-docker-react is docked ui what your todo items memorized via localStorage",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.9",
    "@fortawesome/free-regular-svg-icons": "^5.6.0",
    "@fortawesome/free-solid-svg-icons": "^5.6.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "i": "^0.3.6",
    "npm": "^6.4.1"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "nwb": "0.23.x",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "author": "exilee20c",
  "homepage": "https://github.com/exilee20c/todo-docker-react",
  "license": "MIT",
  "repository": "https://github.com/exilee20c/todo-docker-react",
  "keywords": [
    "todo",
    "localStorage",
    "docked-ui"
  ]
}
